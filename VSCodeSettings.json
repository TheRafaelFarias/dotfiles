{
    "workbench.iconTheme": "material-icon-theme",
    "github.copilot.inlineSuggest.enable": true,
    "github.copilot.enable": {
      "*": true,
      "yaml": false,
      "plaintext": false,
      "markdown": false
    },
    "files.autoSave": "afterDelay",
    "editor.inlineSuggest.enabled": true,
    "workbench.startupEditor": "none",
    "editor.cursorSurroundingLines": 20,
    "json.schemas": [
      {
        "name": "vscode-expo-manifest",
        "url": "file:///home/rafaelfarias/.config/Code%20-%20Insiders/User/globalStorage/bycedric.vscode-expo/manifest-44.0.0.json",
        "fileMatch": ["app.json", "app.config.json"]
      }
    ],
    "editor.minimap.enabled": false,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": true,
      "source.organizeImports": true
    },
    "[typescriptreact]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "redhat.telemetry.enabled": false,
